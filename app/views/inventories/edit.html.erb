<h1>Edit your Inventory!</h1>

<% @inventory.products.each do |product| %>

<ul>
  <%= link_to 'Edit', edit_product_path(product) %><br>
  <%= product.name %><br>
  $<%= product.price %><br>
  <%= product.description %><br>
  For sale by:<%= link_to product.seller.username, users_path(product.seller) %><br>
  <img src="<%= product.image_url %>" alt="<%= product.name %>"><br>
</ul>

<% end %>
