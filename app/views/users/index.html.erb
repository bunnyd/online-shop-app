
<div>
  <h1>You should not be seeing this page...</h1>

<table role='grid' align='center'>
  <thead>
    <caption>User List</caption>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%=user.first_name %></td>
      <td><%=user.last_name %></td>
      <td><%=user.email %></td>

      <td><%= link_to 'Show', user %>
        <%= link_to 'Edit', edit_user_path(user) %> <%= link_to 'Delete', user_path(user), method: :delete %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
